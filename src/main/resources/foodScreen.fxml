<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<AnchorPane id="root-anchorpane" fx:id="pnlRoot" prefHeight="506.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.FXMLControllers.FoodController">
    <children>
        <VBox id="vb-sidebar" fx:id="vbSidebarMain1" layoutX="242.0" layoutY="38.0" prefHeight="130.0" prefWidth="139.0" style="-fx-background-color: green;">
            <children>
                <Label fx:id="label" minHeight="16" minWidth="69" prefHeight="21.0" prefWidth="108.0" text="Select one" />
                <CheckBox fx:id="veganOpt" mnemonicParsing="false" onMouseClicked="#displayVeganMeals" text="Vegan Meal" />
                <CheckBox fx:id="vegOpt" mnemonicParsing="false" onMouseClicked="#displayVegMeals" text="Vegetarian Meal" />
                <CheckBox fx:id="meatOpt" layoutX="10.0" layoutY="52.0" mnemonicParsing="false" onMouseClicked="#displayMeatMeals" text="Meat Meal" />
            </children>
        </VBox>
        <VBox layoutX="242.0" layoutY="183.0" prefHeight="265.0" prefWidth="217.0">
            <children>
                <Label fx:id="mealBoxText" minHeight="16" minWidth="69" prefHeight="37.0" prefWidth="272.0" text="Pick your meal!" />
                <Label fx:id="meal1" layoutX="10.0" layoutY="10.0" minHeight="16" minWidth="69" onMouseClicked="#selectMeal" prefHeight="37.0" prefWidth="324.0" text="Vegetarian Lasagna">
                    <graphic>
                        <Circle fx:id="selectCircle1" fill="#dadfe4" radius="15.0" stroke="BLACK" strokeType="INSIDE" />
                    </graphic>
                </Label>
                <Label fx:id="meal2" layoutX="10.0" layoutY="47.0" minHeight="16" minWidth="69" onMouseClicked="#selectMeal" prefHeight="37.0" prefWidth="324.0" text="Steak">
                    <graphic>
                        <Circle fx:id="selectCircle2" fill="#dadfe4" radius="16.0" stroke="BLACK" strokeType="INSIDE" />
                    </graphic>
                </Label>
                <Label fx:id="meal3" layoutX="10.0" layoutY="84.0" minHeight="16" minWidth="69" onMouseClicked="#selectMeal" prefHeight="37.0" prefWidth="324.0" text="Mozzarella sandwich">
                    <graphic>
                        <Circle fx:id="selectCircle3" fill="#dadfe4" radius="15.0" stroke="BLACK" strokeType="INSIDE" />
                    </graphic>
                </Label>
                <Label fx:id="meal4" layoutX="10.0" layoutY="121.0" minHeight="16" minWidth="69" onMouseClicked="#selectMeal" prefHeight="37.0" prefWidth="319.0" text="Chicken">
                    <graphic>
                        <Circle fx:id="selectCircle4" fill="#dadfe4" radius="15.0" stroke="BLACK" strokeType="INSIDE" />
                    </graphic>
                </Label>
                <Button mnemonicParsing="false" onAction="#getMealPoints" text="Select Meal" />
            <Button mnemonicParsing="false" text="Show Next" />
            <Button mnemonicParsing="false" text="Show Previous" />
            </children>
        </VBox>
    </children>
</AnchorPane>
