<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="AnchorPane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.FXMLControllers.TransportController">
   <children>
      <VBox layoutX="300.0" layoutY="55.0" prefHeight="290.0" prefWidth="150.0" styleClass="VBox">
         <children>
            <Label text="Search your transport!" />
            <TextField />
            <Label text="Or select a category:" />
            <CheckBox mnemonicParsing="false" text="Public transport" />
            <CheckBox mnemonicParsing="false" text="Car" />
            <CheckBox mnemonicParsing="false" text="Emission free" />
         </children></VBox>
      <ScrollPane layoutX="50.0" layoutY="55.0" prefHeight="290.0" prefWidth="205.0" style="-fx-background: none;">
         <content>
            <VBox prefHeight="285.0" prefWidth="200.0" styleClass="EVBox">
               <children>
                  <Label fx:id="transportBoxText" prefHeight="35.0" prefWidth="200.0" text="Pick your transport!" />
                  <Label fx:id="transport1" prefHeight="40.0" prefWidth="200.0" text="Label" />
                  <Label fx:id="transport2" prefHeight="40.0" prefWidth="200.0" text="Label" />
                  <Label fx:id="transport3" prefHeight="40.0" prefWidth="200.0" text="Label" />
                  <Label fx:id="transport4" prefHeight="40.0" prefWidth="200.0" text="Label" />
                  <Button mnemonicParsing="false" style="-fx-border-color: black;" text="Select Transport" />
                  <Button fx:id="upBtn" mnemonicParsing="false" style="-fx-border-color: black;" text="Show Next" />
                  <Button fx:id="downBtn" mnemonicParsing="false" style="-fx-border-color: black;" text="Show Previous" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
   <stylesheets>
      <URL value="@GeneralTemplate.css" />
   </stylesheets>
</AnchorPane>
